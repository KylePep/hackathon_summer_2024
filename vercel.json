{
  "projects": [
    {
      "src": "WorldWarDragon.client/",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "WorldWarDragon/",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "WorldWarDragon/$1" },
    { "src": "/(.*)", "dest": "WorldWarDragon.client/$1" }
  ]
}