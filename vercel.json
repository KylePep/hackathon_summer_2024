{
  "version": 2,
  "builds": [
    {
      "src": "WorldWarDragon/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "WorldWarDragon.client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "/index.js"
    },
    {
      "src": "/.*",
      "dest": "/dist/$1"
    }
  ]
}

